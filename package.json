{
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "gh-pages": "^2.0.0",
    "vuepress": "^0.14.4"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "bash deploy.sh",
    "test": "jest"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "axios": "^0.18.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "chimee": "^0.11.0",
    "chimee-player": "^1.4.7",
    "element-ui": "^2.4.7",
    "escape-string-regexp": "^1.0.5",
    "jest": "^21.2.1",
    "json-server": "^0.14.2",
    "leancloud-realtime": "^4.2.1",
    "leancloud-storage": "^3.11.1",
    "micromatch": "^3.1.10",
    "valine": "^1.3.4",
    "vue-jest": "^3.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/.vuepress/components/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "./coverage/"
  }
}
